{
    "rules": {
        "class-name": true,
        "no-duplicate-variable": true,
        "no-any": true,
        "semicolon": [
            true,
            "always"
        ],
        "no-eval": true,
        "no-duplicate-super": true,
        "import-spacing": true,
        "indent": [
          true,
          "spaces"
        ]
    }
}